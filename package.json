{
  "name": "test-storage",
  "version": "0.42.0",
  "description": "Test Case Management System",
  "license": "GPLv3",
  "author": "Dmitry Zakharov",
  "private": true,
  "keywords": [
    "test management",
    "test case management"
  ],
  "bugs": {
    "url": "https://github.com/pumano/test-storage/issues"
  },
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "build": "ng build --prod",
    "test": "jest",
    "serve": "ng serve --hmr",
    "clean": "rimraf node_modules package-lock.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "extract": "ngx-translate-extract --input ./src --output ./i18n/*.json --clean --sort --format namespaced-json"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm test"
    }
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "dependencies": {
    "@angular/animations": "^11.1.2",
    "@angular/common": "^11.1.2",
    "@angular/compiler": "^11.1.2",
    "@angular/compiler-cli": "^11.1.2",
    "@angular/core": "^11.1.2",
    "@angular/forms": "^11.1.2",
    "@angular/platform-browser": "^11.1.2",
    "@angular/platform-browser-dynamic": "^11.1.2",
    "@angular/pwa": "^0.1101.4",
    "@angular/router": "^11.1.2",
    "@angular/service-worker": "^11.1.2",
    "@auth0/angular-jwt": "^5.0.2",
    "@cds/core": "^5.0.1",
    "@clr/angular": "^5.0.1",
    "@clr/icons": "^5.0.1",
    "@clr/ui": "^5.0.1",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@webcomponents/custom-elements": "^1.4.3",
    "angular2-notifications": "^9.0.0",
    "core-js": "^3.8.3",
    "font-awesome": "^4.7.0",
    "rxjs": "^6.6.3",
    "socket.io-client": "^2.3.1",
    "tailwindcss": "^2.0.2",
    "tslib": "^2.1.0",
    "zone.js": "^0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.0-rc.0",
    "@angular-devkit/schematics": "^11.2.0-rc.1",
    "@angular/cli": "^11.2.0-rc.1",
    "@angular/language-service": "^11.1.2",
    "@babel/preset-env": "^7.12.13",
    "@biesbjerg/ngx-translate-extract": "^7.0.3",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@ngneat/tailwind": "^6.0.3",
    "@types/jest": "^26.0.15",
    "@types/socket.io-client": "^1.4.34",
    "babel-jest": "^26.6.3",
    "faker": "^5.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "postcss": "^8.2.5",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.0.0",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.9",
    "ts-node": "^9.0.0",
    "typescript": "~4.1.3",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/schematics": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0"
  }
}
